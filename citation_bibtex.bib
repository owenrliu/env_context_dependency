@article{Hurst2012,
abstract = {Despite the importance of Pacific cod in Bering Sea fisheries and foodwebs, little is known about the habitat use and the distribution of early life stages. We analysed 6 years of catch data for 0-group Pacific cod in fishery-independent surveys of the Bering Sea shelf. Juvenile cod were most commonly captured on the middle shelf over depths of 50–80 m and were rarely captured north of 588N. Consistently high catches were observed east of the Pribilof Islands and north of Port Moller along the Alaska Peninsula. There was evidence of density-dependent habitat selection at the local scale as the frequency of occurrence increased with regional catch per unit effort. At the basin scale, the southerly distribution of the weak 2009 cohort suggested the possibility of a range contraction for small cohorts. There was no consistent shift in the distribution of juvenile Pacific cod in response to interannual climate variability. These results for Pacific cod contrast with those observed for walleye pollock, which appears to exhibit greater variance in distribution, but are similar to patterns observed for juvenile Atlantic cod. Future work should focus on distribution in nearshore habitats and examine the patterns of dispersal and the connectivity of the Bering Sea and Gulf of Alaska populations.},
author = {Hurst, Thomas P. and Moss, Jamal H. and Miller, Jessica A.},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hurst, Moss, Miller - 2012 - Distributional patterns of 0-group Pacific cod (Gadus macrocephalus) in the eastern Bering SEas under varia.pdf:pdf},
journal = {ICES Journal of Marine Science},
keywords = {cod,haddock,north sea,population diversity,stock recovery,west of scotland,whiting},
number = {2},
pages = {163--174},
title = {{Distributional patterns of 0-group Pacific cod (Gadus macrocephalus) in the eastern Bering SEas under variable recruitment and thermal conditions}},
volume = {69},
year = {2012}
}
@article{Fogarty2012,
abstract = {We developed surplus production models for 12 demersal fish species in$\backslash$nthe Gulf of Maine at the single-species and aggregate-species levels.$\backslash$nSummed single-species production model reference points were higher than$\backslash$nestimates from the aggregate surplus production model. The equilibrium$\backslash$nyield (maximum sustainable yield, MSY) and biomass at MSY (B-MSY) levels$\backslash$nfor the summed single-species production model reference points exceeded$\backslash$nthe aggregate model results by 28.0 and 27.5{\%}, respectively. Biological$\backslash$ninteractions such as predation and competition are potential reasons for$\backslash$ndifferences between the aggregate and summed results. Not accounting for$\backslash$nbiological interactions may result in overly optimistic predictions of$\backslash$nlong-term sustainable yield and unrealistically high estimates of B-MSY.$\backslash$nWe found high concordance between single-species production model$\backslash$nreference points and results from a range of other estimation methods$\backslash$nemployed in assessment of these species, suggesting that the results$\backslash$nfrom the aggregate production model analyses are not artifacts related$\backslash$nto model type. Tests for the effect of environmental variables,$\backslash$nincluding the Atlantic Multidecadal Oscillation (AMO) index, the winter$\backslash$nNorth Atlantic Oscillation (NAO) index, and the Extended Reconstructed$\backslash$nSea Surface Temperature (ERSST) series suggested possible effects of the$\backslash$nNAO at a lag of 0 and the ERSST at a lag of 2 yr based on$\backslash$ncross-correlation analyses. However, further tests proved inconclusive$\backslash$nwhen the covariates were introduced into an extended surplus production$\backslash$nmodel. Given the potential shifts in productivity that can accompany$\backslash$nclimate change, this issue should be periodically re-evaluated, and,$\backslash$nwhere appropriate, a more dynamic approach to setting reference points$\backslash$nshould be pursued.},
author = {Fogarty, M. J. and Overholtz, W. J. and Link, J. S.},
