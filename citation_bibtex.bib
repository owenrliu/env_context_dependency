isbn = {0912-3814},
issn = {09123814},
journal = {Ecological Research},
keywords = {Competition,Indirect effects,Inhibition succession,Intertidal communities,Mytilus,Nucella},
number = {2},
pages = {249--260},
title = {{Mechanisms of successional dynamics: Consumers and the rise and fall of species dominance}},
volume = {17},
year = {2002}
}
