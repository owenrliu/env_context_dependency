file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Walters et al. - 2005 - Possible ecosystem impacts of applying MSY policies from single-species assessment.pdf:pdf},
isbn = {10543139},
