eprint = {2042},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Leibold et al. - 2004 - The metacommunity concept A framework for multi-scale community ecology.pdf:pdf},
isbn = {1461-023X},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Mass effects,Metacommunity,Neutral model,Patch dynamics,Species sorting},
number = {7},
pages = {601--613},
pmid = {137},
title = {{The metacommunity concept: A framework for multi-scale community ecology}},
volume = {7},
year = {2004}
}
