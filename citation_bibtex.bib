issn = {0376-8929},
journal = {Environmental Conservation},
keywords = {channel islands,kelp forests,marine protected areas,northern,sea urchin harvesting,sea urchins,size-mediated interactions,strongylocentrotus franciscanus,strongylocentrotus purpuratus,trophic cascades},
number = {03},
pages = {225--236},
title = {{Reconciling conflict between the direct and indirect effects of marine reserve protection}},
volume = {39},
year = {2012}
}
