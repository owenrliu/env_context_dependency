@book{Bodkin2015,
abstract = {Once numbering perhaps 300,000 throughout nearshore habitats around the Pacific Rim, sea otters were exploited in a 170-year commercial fur harvest that ended early in the twentieth century. Although the average annual harvest was less than about 1.5{\%}, a process of serial depletion resulted in extirpation throughout nearly their entire historic range. By late in the nineteenth century fewer than 2000 animals remained in 13 isolated areas. Subsequent conservation efforts including protection from harvest and translocations resulted in eventual recovery of the species, although large gaps in distribution remain today along western North America, south of Alaska. Currently, about 125,000 individuals occupy their truncated range and most sea otter populations being monitored are experiencing growth rates that are less than rates observed a few decades ago. Despite large tracts of unoccupied habitat there has been little recent change in the overall abundance of sea otters. Behavior and physiology appear to constrain the sea otter to small individual home ranges and result in population structuring at relatively small scales. Varying population trajectories over spatial scales on the order of hundreds of square kilometers or less imply differences in demographic rates at these same scales. Effective conservation will benefit from consideration of those biological attributes specific to individual species that collectively result in unique population structuring properties.},
author = {Bodkin, James L.},
booktitle = {Sea Otter Conservation},
doi = {10.1016/B978-0-12-801402-8.00003-2},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bodkin - 2015 - Historic and Contemporary Status of Sea Otters in the North Pacific.pdf:pdf},
isbn = {9780128016879},
keywords = {Conservation,Growth rates,Harvest,Home range,Population,Sea otter,Translocations},
pages = {43--61},
publisher = {Elsevier Inc.},
title = {{Historic and Contemporary Status of Sea Otters in the North Pacific}},
url = {http://dx.doi.org/10.1016/B978-0-12-801402-8.00003-2},
year = {2015}
}
@incollection{Fay2006a,
abstract = {The timing and extent of the negative population trend in the abundance of the western stock of Steller sea lions has not been geographically uniform. A stochastic metapopulation dynamics model is developed for Steller sea lions. This model allows for geographical differences in factors affecting population processes, and can be parameterized to represent a wide range of hypotheses for the decline in Steller sea lion abundance. Bayesian and maximum likelihood methods are used to fit this model to pup and non-pup count data, age structure samples, and survival estimates. Inferences from model selection criteria highlight the spatial variability in the types of impact deemed to provide most parsimonious representation of the data. Bayesian posteriors for the estimated model parameters show that many combinations of parameter values are able to provide similar fits to the data, even given a specific hypothesis for the decline. This highlights the uncertainty in the precise nature of the impact of these hypotheses. Indeed, while pup production is generally estimated consistently among models, estimates of the size of other com- ponents of the Steller sea lion population (such as total population size) depend greatly on the assumptions regarding the cause of the decline. The results demonstrate that future simulation modeling approaches will require more formal, spatial, and mechanistic descriptions of the manner in which specific hypotheses for the decline affect the population.},
author = {Fay, Gavin and Punt, Andr{\'{e}} E},
booktitle = {Sea Lions of the World},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fay, Punt - 2006 - modeling spatial dynamics of Steller sea lions (Eumetopias jubatus) using maximum likelihood and bayesian methods eva.pdf:pdf},
pages = {405--434},
title = {{modeling spatial dynamics of Steller sea lions (Eumetopias jubatus) using maximum likelihood and bayesian methods: evaluating causes for population decline}},
year = {2006}
}
@article{Walters2008,
abstract = {Surplus production rates predicted by simple biomass dynamics models are generally 23 expected to follow a simple dome-shaped pattern as population size changes, and to show 24 similar trajectories during population decline and recovery. Age-structured models, 25 however, predict substantially lower surplus production rates during population recovery 26 than during decline due to reduced mean fecundity, unless recruitment compensation is 27 very strong. Ecosystem models like Ecosim predict more complex patterns, with reduced 28 production during recoveries due to both age-structure effects and cultivation- 29 depensation effects related to changes in competitor and predator abundances. 30 Production-driven recoveries, where surplus production per biomass is higher during 31 recovery than decline, are predicted in cases where there has been substantial change in 32 overall ecosystem productivity or community structure. 110 case examples illustrate that 33 simple, repeatable relationships between stock size and production are uncommon, and 34 the most common pattern is production-driven change in stock size, where changes in 35 production rate apparently independent of stock size then drive stock increase or 36 decrease. We conclude that non-stationarity in productivity needs to be considered as 37 part of population rebuilding, and that empirical estimates of surplus production may 38 provide insight in this process.},
author = {Walters, Carl J. and Hilborn, Ray and Christensen, Villy},
doi = {10.1139/F08-170},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Walters, Hilborn, Christensen - 2008 - Surplus production dynamics in declining and recovering fish populations.pdf:pdf},
isbn = {0706-652X},
issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {11},
pages = {2536--2551},
title = {{Surplus production dynamics in declining and recovering fish populations}},
volume = {65},
year = {2008}
}
@article{Wootton2005a,
abstract = {Ecologists would like to explain general patterns observed across multi-species communities, such as species-area and abundance-frequency relationships, in terms of the fundamental processes of birth, death and migration underlying the dynamics of all constituent species. The unified neutral theory of biodiversity and related theories based on these fundamental population processes have successfully recreated general species-abundance patterns without accounting for either the variation among species and individuals or resource-releasing processes such as predation and disturbance, long emphasized in ecological theory. If ecological communities can be described adequately without estimating variation in species and their interactions, our understanding of ecological community organization and the predicted consequences of reduced biodiversity and environmental change would shift markedly. Here, I introduce a strong method to test the neutral theory that combines field parameterization of the underlying population dynamics with a field experiment, and apply it to a rocky intertidal community. Although the observed abundance-frequency distribution of the system follows that predicted by the neutral theory, the neutral theory predicts poorly the field experimental results, indicating an essential role for variation in species interactions.},
author = {Wootton, J Timothy},
doi = {10.1038/nature03211},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wootton - 2005 - Field parameterization and experimental test of the neutral theory of biodiversity.pdf:pdf},
isbn = {0028-0836},
issn = {0028-0836},
journal = {Nature},
number = {7023},
pages = {309--312},
pmid = {15662423},
title = {{Field parameterization and experimental test of the neutral theory of biodiversity.}},
volume = {433},
year = {2005}
}
