doi = {10.1371/journal.pone.0043966},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Morissette, Christensen, Pauly - 2012 - Marine Mammal Impacts in Exploited Ecosystems Would Large Scale Culling Benefit Fisheries.pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
number = {9},
title = {{Marine Mammal Impacts in Exploited Ecosystems: Would Large Scale Culling Benefit Fisheries?}},
volume = {7},
