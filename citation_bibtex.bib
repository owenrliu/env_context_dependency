isbn = {1461-0248},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {cross-system comparison,indirect effects,meta-analysis,predation,top-down control},
pages = {785--791},
pmid = {178922200012},
title = {{A cross ecosystem comparison of the strength of trophic cascades}},
url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1461-0248.2002.00381.x/full},
volume = {5},
