journal = {ICES Journal of Marine Science},
keywords = {accepted 16 october 2001,ammodytes marinus,bioenergetics,conservation,fishery management,furness,graham kerr building,harvesting,industrial fishing,institute of biomedical and,life sciences,predator-prey interactions,received 15 june 2001,robert w,top predator,trophic transfer},
