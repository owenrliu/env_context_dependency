journal = {Ecosphere},
keywords = {allee effect,conservation,endangered species,enhydra lutris nereis,keystone species,southern california},
number = {5},
pages = {1--11},
title = {{Sea otters are recolonizing southern California in fits and starts}},
volume = {5},
year = {2014}
}
