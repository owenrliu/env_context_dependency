journal = {Ecological Applications},
keywords = {Channel Islands (California, USA),Disease epidemics,Emerging disease,Host-density threshold for disease transmission,Kelp forest ecosystem,Lobster fishing,Sea urchins,Trophic cascade},
number = {5},
pages = {1566--1573},
pmid = {126},
title = {{Fishing for lobsters indirectly increases epidemics in sea urchins}},
volume = {14},
year = {2004}
}
