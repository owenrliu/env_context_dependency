@article{VanPutten2012a,
abstract = {In the domain of decision-support tools for the management of marine fish resources, considerable attention has been paid to the development of models explaining how fish stocks change over space and time. In most models, fishing effort is assumed to be exogenous and determined by factors such as management. Increasingly, there has been a call for bio-economic models to also account for the dynamics of fishing fleets, recognizing that fishers respond to changing environmental, institutional and economic conditions. A growing literature has sought to explicitly model the endogenous determinants of the capacity of fishing fleets, the intensity of its use and its temporal and spatial allocation across fishing opportunities. We review this literature, focusing on empirical applications of the behavioural models that have been put forward to explain and predict observed fleet dynamics. We find that although economic factors are usually included as a dominant driver in most studies, this is often based on the use of proxy variables for the key economic drivers, for which adequate data are lacking. Also, while many studies acknowledge that social and socialpsychological factors play a significant role in explaining observed fishing behaviour, their inclusion in fishing fleet dynamic models is still very limited. Progress in this domain can only be achieved via the development of multidisciplinary research programmes focusing on applied quantitative analysis of the drivers of fishing fleet dynamics.},
author = {{Van Putten}, Ingrid E. and Kulmala, Soile and Th{\'{e}}baud, Olivier and Dowling, Natalie and Hamon, Katell G. and Hutton, Trevor and Pascoe, Sean},
doi = {10.1111/j.1467-2979.2011.00430.x},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Van Putten et al. - 2012 - Theories and behavioural drivers underlying fleet dynamics models.pdf:pdf},
isbn = {1467-2979},
issn = {14672960},
journal = {Fish and Fisheries},
keywords = {Bio-economic models,Fisher behaviour,Fishery modelling,Fleet dynamics,Review},
number = {2},
pages = {216--235},
pmid = {20123156461},
title = {{Theories and behavioural drivers underlying fleet dynamics models}},
volume = {13},
year = {2012}
}
@article{Szpak2012,
abstract = {We examined the isotopic composition ($\delta$ 13C and $\delta$ 15N) of sea otter (Enhydra lutris) bone collagen from ten late Holocene (ca. 5200 years BP-AD 1900) archaeological sites in northern British Columbia (BC), Canada. Because sea otters are now extinct from much of this region and have not fully recolonized their former range (e.g. Haida Gwaii and most of northern BC) these data represent an important first step towards better understanding sea otter foraging ecology in BC. The isotopic data suggest a diet composed primarily of benthic invertebrates, with a very low reliance on epibenthic fish. There is very low isotopic and thus inter-individual dietary variability in Holocene BC sea otters during the late Holocene. Furthermore, zooarchaeological abundance data suggest that otters represented a widespread and significant focus of aboriginal hunting practices on the northern BC coast during the mid- to late-Holocene. The consistent reliance on a small number of low-trophic level prey and limited dietary variability in Holocene BC sea otters may reflect top-down impacts on otter populations by aboriginal peoples. As part of our assessment of sea otter diet, we review trophic discrimination factors ($\Delta$ 13C and $\Delta$ 15N) for bone collagen from published literature and find marked variability, with mean values of +3.7 ± 1.6 for $\Delta$ 13C (n = 21) and +3.6 ± 1.3 for $\Delta$ 15N (n = 15). {\textcopyright} 2011 Elsevier Ltd.},
author = {Szpak, Paul and Orchard, Trevor J. and McKechnie, Iain and Gr{\"{o}}cke, Darren R.},
doi = {10.1016/j.jas.2011.12.006},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Szpak et al. - 2012 - Historical ecology of late Holocene sea otters (Enhydra lutris) from northern British Columbia Isotopic and zooarc.pdf:pdf},
isbn = {0305-4403},
issn = {03054403},
journal = {Journal of Archaeological Science},
keywords = {British Columbia,Ecology of fear,Historical ecology,Quantifying isotopic variability,Sea otter,Stable isotopes,Zooarchaeology},
number = {5},
pages = {1553--1571},
publisher = {Elsevier Ltd},
title = {{Historical ecology of late Holocene sea otters (Enhydra lutris) from northern British Columbia: Isotopic and zooarchaeological perspectives}},
url = {http://dx.doi.org/10.1016/j.jas.2011.12.006},
volume = {39},
year = {2012}
}
