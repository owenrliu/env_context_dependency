issn = {03043800},
journal = {Ecological Modelling},
keywords = {Bistability,Harvesting,Hysteresis,Sea-urchin},
pages = {1--5},
publisher = {Elsevier B.V.},
title = {{Predator exploitation and sea urchin bistability: Consequence on benthic alternative states}},
url = {http://dx.doi.org/10.1016/j.ecolmodel.2016.10.021},
volume = {344},
year = {2017}
}
