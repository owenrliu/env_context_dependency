isbn = {1365-2656},
issn = {13652656},
journal = {Journal of Animal Ecology},
keywords = {Behaviour-mediated top-down effects,Non-consumptive predator effects,Predator-induced habitat selection,Predator-prey interactions},
number = {5},
pages = {1286--1298},
pmid = {25976520},
title = {{Spatial distribution of fishes in a Northwest Atlantic ecosystem in relation to risk of predation by a marine mammal}},
