@article{Provost2017,
abstract = {The combination of ocean warming and acidification brings an uncertain$\backslash$nfuture to kelp forests that occupy the warmest parts of their range.$\backslash$nThese forests are not only subject to the direct negative effects of$\backslash$nocean climate change, but also to a combination of unknown indirect$\backslash$neffects associated with changing ecological landscapes. Here, we used$\backslash$nmesocosm experiments to test the direct effects of ocean warming and$\backslash$nacidification on kelp biomass and photosynthetic health, as well as$\backslash$nclimate-driven disparities in indirect effects involving key consumers$\backslash$n(urchins and rock lobsters) and competitors (algal turf). Elevated water$\backslash$ntemperature directly reduced kelp biomass, while their turf-forming$\backslash$ncompetitors expanded in response to ocean acidification and declining$\backslash$nkelp canopy. Elevated temperatures also increased growth of urchins and,$\backslash$nconcurrently, the rate at which they thinned kelp canopy. Rock lobsters,$\backslash$nwhich are renowned for keeping urchin populations in check, indirectly$\backslash$nintensified negative pressures on kelp by reducing their consumption of$\backslash$nurchins in response to elevated temperature. Overall, these results$\backslash$nsuggest that kelp forests situated towards the low-latitude margins of$\backslash$ntheir distribution will need to adapt to ocean warming in order to$\backslash$npersist in the future. What is less certain is how such adaptation in$\backslash$nkelps can occur in the face of intensifying consumptive (via ocean$\backslash$nwarming) and competitive (via ocean acidification) pressures that affect$\backslash$nkey ecological interactions associated with their persistence. If such$\backslash$nindirect effects counter adaptation to changing climate, they may erode$\backslash$nthe stability of kelp forests and increase the probability of regime$\backslash$nshifts from complex habitat-forming species to more simple habitats$\backslash$ndominated by algal turfs.},
