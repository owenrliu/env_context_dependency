file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chase et al. - 2002 - The interaction between predation and competition A review and synthesis.pdf:pdf},
isbn = {1461-023X},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Coexistence,Diversity,Herbivory,Interaction strength,Interspecific competition,Predation},
number = {2},
pages = {302--315},
pmid = {175119000019},
title = {{The interaction between predation and competition: A review and synthesis}},
volume = {5},
year = {2002}
}
