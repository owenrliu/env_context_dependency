issn = {11758716},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
pages = {532--550},
pmid = {23385843},
title = {{Life history schedule and periodic recruitment of female snow crab (Chionoecetes opilio) in the eastern Bering Sea}},
volume = {69},
year = {2012}
}
