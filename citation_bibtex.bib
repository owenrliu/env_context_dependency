isbn = {0171-8630},
issn = {0171-8630 1616-1599},
journal = {Marine Ecology Progress Series},
keywords = {biological reference points,competition,ecosystem-based fishery management,models,multiple objectives,of the publisher,permitted without written consent,predation,resale or republication not,species complexes,surplus production},
number = {Watt 1968},
pages = {275--+},
title = {{Assembly rules for aggregate-species production models: simulations in support of management strategy evaluation}},
