@article{Parnell2010,
abstract = {The nutrient climate on the inner shelf off southern California changed$\backslash$nmarkedly across the 1976-1977 North Pacific climate regime shift. With$\backslash$nrespect to giant kelp (Macrocystis pyrifera) canopies off southern$\backslash$nCalifornia, the nitrate climate shifted from relatively replete$\backslash$nconditions prior to the regime shift to depleted conditions afterward,$\backslash$nand the dynamics of 14 giant kelp forests appeared to change as a$\backslash$nresult. The response of giant kelp to nutrient-replete years before the$\backslash$nregime shift was dampened compared to their response afterward. The$\backslash$nsensitivity of these kelp-forest canopies to nutrient limitation appears$\backslash$nto have increased since the regime shift. This intensification of$\backslash$nphysical control after 1977 is evident in the strong correlation of$\backslash$nseawater density (sigma(t)) and M. pyrifera density. The linear fit of$\backslash$nthe percent of time the 25.1 sigma(t) isopycnal bathes the inner shelf,$\backslash$naccounted for similar to 71{\%} of the variability in kelp density off$\backslash$nPoint Loma, and the median depth of this isopycnal has deepened similar$\backslash$nto 5 m since the regime shift. The wave climate also intensified$\backslash$nbeginning in the early 1970s. The dampened kelp response prior to the$\backslash$nregime shift was likely due to greater biological control of kelp$\backslash$ncanopies via consumer and competitive processes (i.e., biological$\backslash$nmodulation) or decreased physical control at possibly many trophic$\backslash$nlevels. Our results suggest that the response of kelp forests to El Nino$\backslash$nSouthern Oscillation events is mediated by lower frequency climate modes$\backslash$nthat may modulate the regulatory importance of biological and physical$\backslash$nprocesses on giant kelp.},
author = {Parnell, P. Ed and Miller, Eric F. and Lennert-Cody, Cleridy E. and Dayton, Paul K. and Carter, Melissa L. and Stebbins, Timothy D.},
doi = {10.4319/lo.2010.55.6.2686},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Parnell et al. - 2010 - The response of giant kelp (Macrocystis pyrifera) in southern California to low-frequency climate forcing.pdf:pdf},
isbn = {0024-3590},
issn = {0024-3590},
journal = {Limnology and Oceanography},
number = {6},
pages = {2686--2702},
title = {{The response of giant kelp (Macrocystis pyrifera) in southern California to low-frequency climate forcing}},
url = {http://www.aslo.info/lo/toc/vol{\_}55/issue{\_}6/2686.pdf},
volume = {55},
year = {2010}
}
@article{Ling2009,
abstract = {A key consideration in assessing impacts of climate change is the possibility of synergistic effects with other human-induced stressors. In the ocean realm, climate change and overfishing pose two of the greatest challenges to the structure and functioning of marine ecosystems. In eastern Tasmania, temperate coastal waters are warming at approximately four times the global ocean warming average, representing the fastest rate of warming in the Southern Hemisphere. This has driven range extension of the ecologically important long-spined sea urchin (Centrostephanus rodgersii), which has now commenced catastrophic overgrazing of productive Tasmanian kelp beds leading to loss of biodiversity and important rocky reef ecosystem services. Coincident with the overgrazing is heavy fishing of reef-based predators including the spiny lobster Jasus edwardsii. By conducting experiments inside and outside Marine Protected Areas we show that fishing, by removing large predatory lobsters, has reduced the resilience of kelp beds against the climate-driven threat of the sea urchin and thus increased risk of catastrophic shift to widespread sea urchin barrens. This shows that interactions between multiple human-induced stressors can exacerbate nonlinear responses of ecosystems to climate change and limit the adaptive capacity of these systems. Management actions focused on reducing the risk of catastrophic phase shift in ecosystems are particularly urgent in the face of ongoing warming and unprecedented levels of predator removal from the world's oceans.},
author = {Ling, S D and Johnson, C R and Frusher, S D and Ridgway, K R},
doi = {10.1073/pnas.0907529106},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ling et al. - 2009 - Overfishing reduces resilience of kelp beds to climate-driven catastrophic phase shift.pdf:pdf},
isbn = {0027-8424},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Biodiversity,Climate Change,Ecosystem,Fishes,Food Chain,Humans,Kelp,Models, Biological,Palinuridae,Palinuridae: physiology,Predatory Behavior,Sea Urchins,Sea Urchins: physiology,Tasmania},
number = {52},
pages = {22341--5},
pmid = {20018706},
title = {{Overfishing reduces resilience of kelp beds to climate-driven catastrophic phase shift.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2793314{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {106},
year = {2009}
}
@article{Murawski2010,
abstract = {Murawski, S. A., Steele, J. H., Taylor, P., Fogarty, M. J., Sissenwine, M. P., Ford, M., and Suchman, C. 2010. Why compare marine ecosystems? - ICES Journal of Marine Science, 67: 1-9. Effective marine ecosystem-based management (EBM) requires understanding the key processes and relationships controlling the aspects of biodiversity, productivity, and resilience to perturbations. Unfortunately, the scales, complexity, and non-linear dynamics that characterize marine ecosystems often confound managing for these properties. Nevertheless, scientifically derived decision-support tools (DSTs) are needed to account for impacts resulting from a variety of simultaneous human activities. Three possible methodologies for revealing mechanisms necessary to develop DSTs for EBM are: (i) controlled experimentation, (ii) iterative programmes of observation and modelling ("learning by doing"), and (iii) comparative ecosystem analysis. We have seen that controlled experiments are limited in capturing the complexity necessary to develop models of marine ecosystem dynamics with sufficient realism at appropriate scales. Iterative programmes of observation, model building, and assessment are useful for specific ecosystem issues but rarely lead to generally transferable products. Comparative ecosystem analyses may be the most effective, building on the first two by inferring ecosystem processes based on comparisons and contrasts of ecosystem response to human-induced factors. We propose a hierarchical system of ecosystem comparisons to include within-ecosystem comparisons (utilizing temporal and spatial changes in relation to human activities), within-ecosystem-type comparisons (e.g. coral reefs, temperate continental shelves, upwelling areas), and cross-ecosystem-type comparisons (e.g. coral reefs vs. boreal, terrestrial vs. marine ecosystems). Such a hierarchical comparative approach should lead to better understanding of the processes controlling biodiversity, productivity, and the resilience of marine ecosystems. In turn, better understanding of these processes will lead to the development of increasingly general laws, hypotheses, functional forms, governing equations, and broad interpretations of ecosystem responses to human activities, ultimately improving DSTs in support of EBM.},
author = {Murawski, S a and Steele, J H and Taylor, P and Fogarty, M J and Sissenwine, M P and Ford, M and Suchman, C},
doi = {10.1093/icesjms/fsp221},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Murawski et al. - 2010 - Why compare marine ecosystems.pdf:pdf},
isbn = {1054-3139},
issn = {10959289},
journal = {ICES Journal of Marine Science},
keywords = {1315 east west highway,a,accepted 28 july 2009,advance access publication 30,august 2009,comparative marine ecosystem analysis,decision-support tools,eam,ebm,ecological modelling,ecosystem approaches to,ecosystem-based management,ford,h,j,management,md 20910,murawski and m,national marine fisheries service,received 15 january 2009,s,silver spring,steele and,usa},
number = {1},
pages = {1--9},
title = {{Why compare marine ecosystems?}},
url = {http://icesjms.oxfordjournals.org/cgi/content/abstract/67/1/1},
volume = {67},
year = {2010}
}
