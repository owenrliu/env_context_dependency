doi = {10.1006/jmsc.2000.0586},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pastoors, Rijnsdorp, Van Beek - 2000 - Effects of a partially closed area in the North Sea (“plaice box”) on stock development of plaice.pdf:pdf},
