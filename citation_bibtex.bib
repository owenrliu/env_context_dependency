isbn = {0000000154871},
issn = {13652656},
journal = {Journal of Animal Ecology},
keywords = {Pycnopodia helianthoides,Strongylocentrotus purpuratus,ecosystem shift,global climate change,kelp forest,sea urchin barren,structural equation modelling,top-down control},
number = {3},
