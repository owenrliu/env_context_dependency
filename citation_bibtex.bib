@article{Lindenmayer2009a,
abstract = {Long-term research and monitoring can provide important ecological insights and are crucial for the improved management of ecosystems and natural resources. However, many long-term research and monitoring programs are either ineffective or fail completely owing to poor planning and/or lack of focus. Here we propose the paradigm of adaptive monitoring, which aims to resolve many of the problems that have undermined previous attempts to establish long-term research and monitoring. This paradigm is driven by tractable questions, rigorous statistical design at the outset, a conceptual model of the ecosystem or other entity being examined and a human need to know about ecosystem change. An adaptive monitoring framework enables monitoring programs to evolve iteratively as new information emerges and research questions change. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
author = {Lindenmayer, David B. and Likens, Gene E.},
doi = {10.1016/j.tree.2009.03.005},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lindenmayer, Likens - 2009 - Adaptive monitoring a new paradigm for long-term research and monitoring.pdf:pdf},
isbn = {0169-5347},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
number = {9},
pages = {482--486},
pmid = {19409648},
title = {{Adaptive monitoring: a new paradigm for long-term research and monitoring}},
volume = {24},
year = {2009}
}
@article{Gislason1993,
abstract = {A multispecies model (MSFOR) is used to predict the relative change in$\backslash$nequilibrium yield and spawning stock biomass (SSB) of commercially$\backslash$nimportant fish stocks in the North Sea resulting from a reduction in the$\backslash$nfishing mortality generated by the roundfish fishery. Because predation$\backslash$nmortality is a function of the abundance of prey and predators the$\backslash$nresults will depend on recruitment. Assuming recruitment to be$\backslash$nindependent of stock sizes the effect of changes in recruitment is$\backslash$nstudied by repeating the prediction, at all possible combinations of$\backslash$n+/-50{\%} changes in predator and prey recruitment levels, All of the$\backslash$npredictions result in a relative increase in the SSB of Atlantic cod$\backslash$n(Gadus morhua) and saithe (Pollachius virens) and in a relative decrease$\backslash$nin the SSB of Atlantic herring (Clupea harengus) and Norway pout$\backslash$n(Trisopterus esmarkii). In these cases the sign of the relative change$\backslash$nis robust to recruitment changes. However, tor haddock (Melanogrammus$\backslash$naeglefinus), sprat (Sprattus sprattus), and sandeel (Ammodytes marinus)$\backslash$nthe relative change in SSB is found to be either positive or negative$\backslash$ndepending on the level ot recruitment. The predictions for haddock are$\backslash$nhighly sensitive to change in the level of saithe recruitment.},
author = {Gislason, Henrik},
doi = {10.1139/f93-256},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gislason - 1993 - Effect of Changes in Recruitment Levels on Multispecies Long-Term Predictions.pdf:pdf},
issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {11},
pages = {2315--2322},
title = {{Effect of Changes in Recruitment Levels on Multispecies Long-Term Predictions}},
volume = {50},
year = {1993}
}
@article{VanLeeuwen2013,
abstract = {Catastrophic collapses of top predators have revealed trophic cascades and community structuring by top-down control. When populations fail to recover after a collapse, this may indicate alternative stable states in the system. Overfishing has caused several of the most compelling cases of these dynamics, and in particular Atlantic cod stocks exemplify such lack of recovery. Often, competition between prey species and juvenile predators is hypothesized to explain the lack of recovery of predator populations. The predator is then considered to compete with its prey for one resource when small and to subsequently shift to piscivory. Yet predator life history is often more complex than that, including multiple ontogenetic diet shifts. Here we show that no alternative stable states occur when predators in an intermediate life stage feed on an additional resource (exclusive to the predator) before switching to piscivory, because predation and competition between prey and predator do not simultaneously structure community dynamics. We find top-down control by the predator only when there is no feedback from predator foraging on the additional resource. Otherwise, the predator population dynamics are governed by a bottleneck in individual growth occurring in the intermediate life stage. Therefore, additional resources for predators may be beneficial or detrimental for predator population growth and strongly influence the potential for top-down community control.},
author = {van Leeuwen, Anieke and Huss, Magnus and G{\aa}rdmark, Anna and Casini, Michele and Vitale, Francesca and Hjelm, Joakim and Persson, Lennart and de Roos, Andr{\'{e}} M},
doi = {10.1086/670614},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/van Leeuwen et al. - 2013 - Predators with multiple ontogenetic niche shifts have limited potential for population growth and top-down c.pdf:pdf},
isbn = {1537-5323},
issn = {1537-5323},
journal = {The American Naturalist},
keywords = {Animals,Biological,Ecosystem,Fishes,Fishes: physiology,Food Chain,Gadus morhua,Gadus morhua: physiology,Models,Population Dynamics,Population Growth,Predatory Behavior},
number = {1},
pages = {53--66},
pmid = {23778226},
title = {{Predators with multiple ontogenetic niche shifts have limited potential for population growth and top-down control of their prey.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23778226},
volume = {182},
year = {2013}
}
@article{Peters2007,
abstract = {Abstract  Cross–scale interactions refer to processes at one spatial or temporal scale interacting with processes at another scale to result in nonlinear dynamics with thresholds. These interactions change the pattern–process relationships across scales such that fine-scale processes can influence a broad spatial extent or a long time period, or broad-scale drivers can interact with fine-scale processes to determine system dynamics. Cross–scale interactions are increasing recognized as having important influences on ecosystem processes, yet they pose formidable challenges for understanding and forecasting ecosystem dynamics. In this introduction to the special feature, “Cross–scale interactions and pattern–process relationships”, we provide a synthetic framework for understanding the causes and consequences of cross–scale interactions. Our framework focuses on the importance of transfer processes and spatial heterogeneity at intermediate scales in linking fine- and broad-scale patterns and processes. Transfer processes and spatial heterogeneity can either amplify or attenuate system response to broad-scale drivers. Providing a framework to explain cross–scale interactions is an important step in improving our understanding and ability to predict the impacts of propagating events and to ameliorate these impacts through proactive measures.},
author = {Peters, Debra P C and Bestelmeyer, Brandon T. and Turner, Monica G.},
doi = {10.1007/s10021-007-9055-6},
file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Peters, Bestelmeyer, Turner - 2007 - Cross-scale interactions and changing pattern-process relationships Consequences for system dynamic.pdf:pdf},
isbn = {1432-9840},
issn = {14329840},
journal = {Ecosystems},
keywords = {Ecological surprises,Landscape ecology,Propagating events,Spatial heterogeneity,Transfer processes},
number = {5},
pages = {790--796},
title = {{Cross-scale interactions and changing pattern-process relationships: Consequences for system dynamics}},
volume = {10},
year = {2007}
}
