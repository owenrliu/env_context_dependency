file = {:C$\backslash$:/Users/Owen.Liu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ling et al. - 2009 - Climate-driven range extension of a sea urchin Inferring future trends by analysis of recent population dynamics.pdf:pdf},
isbn = {13541013},
