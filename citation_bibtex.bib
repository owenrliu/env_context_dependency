issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Bayesian,Decision theory,Fisheries management,Gaussian processes,Non-parametric optimal control,Structural uncertainty},
number = {1801},
pages = {8--11},
pmid = {25567644},
title = {{Avoiding tipping points in fisheries management through gaussian process dynamic programming}},
